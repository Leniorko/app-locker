<Window x:Class="AppLocker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppLocker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="MainInfo" Margin="500,10,0,10"/>
        <Button x:Name="GetInstalled" Margin="0,10,686,10" Click="Button_Click" AutomationProperties.Name="start" AutomationProperties.IsRowHeader="True" Content="GetInstalled"></Button>
        <Button x:Name="GetProcesses" Margin="119,10,534,10" Content="GetProcesses" Click="GetProcesses_Click"></Button>
    </Grid>
</Window>
